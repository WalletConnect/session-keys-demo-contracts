#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/hardhat@2.22.1_typescript@5.4.2/node_modules/hardhat/internal/cli/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/hardhat@2.22.1_typescript@5.4.2/node_modules/hardhat/internal/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/hardhat@2.22.1_typescript@5.4.2/node_modules/hardhat/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/hardhat@2.22.1_typescript@5.4.2/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/hardhat@2.22.1_typescript@5.4.2/node_modules/hardhat/internal/cli/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/hardhat@2.22.1_typescript@5.4.2/node_modules/hardhat/internal/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/hardhat@2.22.1_typescript@5.4.2/node_modules/hardhat/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/hardhat@2.22.1_typescript@5.4.2/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../hardhat@2.22.1_typescript@5.4.2/node_modules/hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../../../../../../hardhat@2.22.1_typescript@5.4.2/node_modules/hardhat/internal/cli/bootstrap.js" "$@"
fi
