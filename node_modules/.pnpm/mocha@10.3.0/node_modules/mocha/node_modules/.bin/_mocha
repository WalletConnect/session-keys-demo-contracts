#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/mocha@10.3.0/node_modules/mocha/bin/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/mocha@10.3.0/node_modules/mocha/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/mocha@10.3.0/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/mocha@10.3.0/node_modules/mocha/bin/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/mocha@10.3.0/node_modules/mocha/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/mocha@10.3.0/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/_mocha" "$@"
else
  exec node  "$basedir/../../bin/_mocha" "$@"
fi
