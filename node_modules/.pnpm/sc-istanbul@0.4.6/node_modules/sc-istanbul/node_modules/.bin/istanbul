#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules/sc-istanbul/lib/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules/sc-istanbul/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules/sc-istanbul/lib/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules/sc-istanbul/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules:/Users/filmakarov/_dev/walletcon-permissions-contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
